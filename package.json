  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "npm run build:clean && npm run build:prerender && npm run build:prod && npm run build:post ",
    "build:clean": "rimraf build",
    "build:prod": "NODE_ENV=production webpack --color --config config/webpack --progress --display-modules",
    "build:post": "NODE_ENV=production node config/scripts",
    "build:prerender": "NODE_ENV=production SERVER=1 webpack --config config/webpack",
    "build:dll": "NODE_ENV=dll webpack --display-chunks --color --display-modules --config config/webpack",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.scss'",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:staged": "lint-staged",
    "start": "concurrently --kill-others 'npm run start:dev' 'npm run start:render' 'npm run start:prerender'",
    "start:dev": "NODE_ENV=development node config/dev-server",
    "start:prerender": "NODE_ENV=development SERVER=1 webpack --config config/webpack",
    "start:render": "NODE_ENV=development node config/server"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "eslint",
    "*.scss": "stylelint"
  },
